{
  "name": "react-chrome-boilerplate",
  "version": "1.0.0",
  "description": "This is React chrome extension boilerplate",
  "main": "index.js",
  "scripts": {
    "commit": "git add -A && git-cz",
    "start": "node webpack/server",
    "build": "NODE_ENV=production rm -rf crx && webpack --config webpack/webpack.config.prod.js && node webpack/package",
    "publish": "npm run build && node webpack/publish"
  },


  "extensionId": "This is your extension id",
  "clientId": "This is your client id",
  "clientSecret": "This is your client secret",
  "refreshToken": "This is your refresh token",

  
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-core": "^6.14.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.2.10",
    "babel-polyfill": "*",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chrome-webstore-upload": "^0.2.1",
    "commitizen": "^2.9.5",
    "cz-conventional-changelog": "^2.1.0",
    "ejs": "^2.5.5",
    "execa": "^0.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "fs-extra": "^6.0.1",
    "glob": "^7.1.1",
    "html-loader": "^0.5.5",
    "img-loader": "^3.0.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "rucksack-css": "^1.0.2",
    "string-loader": "0.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.14.0",
    "webpack-merge": "^4.1.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "lodash": "^4.13.1",
    "classnames": "^2.2.5",
    "material-ui": "^0.20.1",
    "mfetch": "^0.2.23",
    "pubsub-js": "^1.5.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  }
}
